#!/bin/bash
./raytrace jupiter_zoom.png 700 500 Jupiter 2021-12-10T23:12:00Z -v -f -s || exit 1
./raytrace jupiter_full.png 1400 150 Jupiter 2021-12-10T23:20:00Z -v -f -s || exit 1
./raytrace venus.png 200 200 Venus 2021-12-28T00:00:00Z -v -z0.8 || exit 1
./raytrace saturn.png 800 350 Saturn 2021-12-28T00:00:00Z -s -v || exit 1
exit 0
