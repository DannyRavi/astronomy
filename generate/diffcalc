#!/bin/bash

./ctest $1 diff 0.0 temp/c_check.txt dotnet/csharp_test/csharp_check.txt || exit $?
./ctest $1 diff 2.7e-14 temp/{c,js}_check.txt || exit $?
./ctest $1 diff 1.6e-16 temp/{c,py}_check.txt || exit $?
./ctest $1 diff 2.7e-14 temp/{js,py}_check.txt || exit $?

echo ""
echo "diffcalc: PASS"
exit 0
